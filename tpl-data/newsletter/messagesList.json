{
	"name": "Messages list",
	"description": "example description",
	"#_collection_#":[
		{
			"f_fieldset":"true",
			"name":"fieldset#_index_#",
			"options":[
				{ 
					"f_label":"true",	
					"id":"label#_index_#",					
					"value":"#_value.id_#",
					"flex":"0 0 5%"
				},
				{ 
					"f_label":"true",	
					"id":"label#_index_#",					
					"value":"#_value.datelastsent_#",
					"flex":"0 0 5%"
				},
				{ 
					"f_label":"true",	
					"id":"label#_index_#",					
					"value":"#_value.status_#",
					"flex":"0 0 5%"
				},
				{ 
					"f_text":"true",
					"label":"Example label from last tab",
					"name":"postname#_index_#",
					"value":"#_value.subject_#",
					"flex":"0 0 45%"
				},
				{ 
					"id":"#_value.id_#",
					"f_action":"true",										
					"label":"Resend message",
					"flex":"0 0 20%",
					"class":"full-width",
					"action":"sGETinit_newsletter",
					"template":"mainFormTemplate",
					"viewDataTarget":"editMessage",
					"urldata":"id%3D#_value.id_#"
				}
				
			]
		}
	],
	"options":[
		{
			"f_fieldset":"true",
			"name":"addMessage",
			"options":[
				{ 
					"f_action":true,
					"label":"Create new newsletter",
					"class":"btn-primary btn-lg full-width",
					"flex":"0 0 100%",
					"action":"sGETinit_newsletter",
					"template":"mainFormTemplate",
					"viewDataTarget":"addMessage"
				}
			]
		}

	],
	"cssframework":{
		"group":"form-group",
		"input":"form-input",
		"select":"form-select"
	}
}